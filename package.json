{
  "name": "vieb",
  "productName": "Vieb",
  "version": "10.0.0-dev",
  "description": "Vim Inspired Electron Browser",
  "main": "app/index.js",
  "scripts": {
    "dev": "electron app --datafolder=./ViebData/ --config-order=datafolder-only",
    "fix": "eslint --fix app .eslintrc.js build.js",
    "lint": "eslint app .eslintrc.js build.js",
    "start": "electron app",
    "test": "TZ=UTC jest --testEnvironment jsdom --coverage --collectCoverageFrom 'app/**/*.js' -u",
    "test:all": "npm run test && npm run lint && echo 'All good :)'"
  },
  "repository": "https://github.com/Jelmerro/Vieb",
  "homepage": "https://vieb.dev",
  "keywords": [
    "Vim",
    "Electron",
    "Browser",
    "Internet"
  ],
  "author": "Jelmer van Arnhem",
  "email": "Jelmerro@users.noreply.github.com",
  "funding": "https://github.com/sponsors/Jelmerro/",
  "license": "GPL-3.0-or-later",
  "devDependencies": {
    "@types/marked": "4.0.8",
    "electron": "24.0.0",
    "electron-builder": "24.2.0",
    "eslint": "8.37.0",
    "eslint-plugin-sort-keys": "2.3.5",
    "jest": "29.5.0",
    "jest-environment-jsdom": "29.5.0",
    "terser-webpack-plugin": "5.3.7",
    "typescript": "5.0.4",
    "webpack": "5.78.0",
    "webpack-cli": "5.0.1",
    "webpack-node-externals": "3.0.0"
  },
  "dependencies": {
    "@cliqz/adblocker-electron": "1.26.5",
    "@cliqz/adblocker-electron-preload": "1.26.5",
    "@mozilla/readability": "0.4.4",
    "darkreader": "4.9.58",
    "highlight.js": "11.7.0",
    "jsdom": "21.1.1",
    "marked": "4.3.0",
    "picomatch": "2.3.1"
  }
}
